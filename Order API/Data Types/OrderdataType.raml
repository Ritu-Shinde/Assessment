#%RAML 1.0 DataType

type: object
properties:
  OrderID?: number
  Name: string
  OrderDate: datetime-only
  MobileNo: number
  Address:
    type: object
    properties:
      street: string
      city: string
      zipcode: number
  Products:
    type: object
    properties:
      Product1: string
      Price1: number
      Product2: string
      Price2: number
      Product3: string
      Price3: number

